{
    "manifest_version": 3,
    "name": "Sentry Audit Automator 3000",
    "description": "Saves you time in knowing how your accounts are doing.",
    "version": "0.01",
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": [
          "popup.js",
          "engine.js"
          ]
        }], 
    "icons": {
          "16": "images/pup_16.png",
          "32": "images/pup_32.png",
          "48": "images/pup_48.png",
          "64": "images/pup_64.png"
    },
    "host_permissions": [
      "https://*.sentry.io/*"
    ],
    "permissions": [
      "tabGroups",
      "cookies",
      "storage",
      "tabs"
    ]
  }